{
	"name": "personal-website",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"build:ci": "vite build --mode production",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:ci": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --output machine --fail-on-warnings",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"lint:eslint": "eslint .",
		"format": "prettier . --write . --cache",
		"format:check": "prettier --plugin-search-dir . --check ."
	},
	"devDependencies": {
		"@fortawesome/free-brands-svg-icons": "^6.4.2",
		"@fortawesome/free-solid-svg-icons": "^6.4.2",
		"@sveltejs/adapter-cloudflare": "^2.3.3",
		"@sveltejs/adapter-static": "^2.0.3",
		"@sveltejs/kit": "^1.27.6",
		"@types/node": "^20.10.2",
		"@typescript-eslint/eslint-plugin": "^6.16.0",
		"@typescript-eslint/parser": "^6.16.0",
		"autoprefixer": "^10.4.16",
		"cssnano": "^6.0.1",
		"eslint": "^8.55.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-svelte": "^2.35.1",
		"postcss": "^8.4.31",
		"prettier": "^3.0.3",
		"prettier-plugin-svelte": "^3.0.3",
		"svelte": "^4.2.8",
		"svelte-awesome": "^3.2.1",
		"svelte-check": "^3.6.0",
		"svelte-eslint-parser": "^0.33.1",
		"tailwindcss": "^3.3.6",
		"tslib": "^2.6.2",
		"typescript": "^5.3.2",
		"vite": "^4.5.0",
		"vite-plugin-tailwind-purgecss": "^0.2.0",
		"vitest": "^0.34.6"
	},
	"type": "module",
	"dependencies": {
		"@skeletonlabs/skeleton": "^2.5.0",
		"@skeletonlabs/tw-plugin": "^0.2.4"
	}
}
