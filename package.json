{
	"name": "personal-website",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"build:ci": "vite build --mode production",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:ci": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --output machine --fail-on-warnings",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"lint:eslint": "eslint .",
		"format": "prettier . --write . --cache",
		"format:check": "prettier --plugin-search-dir . --check ."
	},
	"devDependencies": {
		"@fortawesome/free-brands-svg-icons": "^6.4.2",
		"@fortawesome/free-solid-svg-icons": "^6.4.0",
		"@skeletonlabs/skeleton": "^1.11.0",
		"@sveltejs/adapter-cloudflare": "^2.3.2",
		"@sveltejs/adapter-static": "^2.0.3",
		"@sveltejs/kit": "^1.22.5",
		"@types/node": "^20.4.10",
		"@typescript-eslint/eslint-plugin": "^5.61.0",
		"@typescript-eslint/parser": "^5.61.0",
		"autoprefixer": "^10.4.14",
		"cssnano": "^6.0.1",
		"eslint": "^8.47.0",
		"eslint-config-prettier": "^8.10.0",
		"eslint-plugin-svelte": "^2.32.4",
		"postcss": "^8.4.27",
		"prettier": "^3.0.1",
		"prettier-plugin-svelte": "^3.0.3",
		"svelte": "^4.1.2",
		"svelte-awesome": "^3.2.1",
		"svelte-check": "^3.5.0",
		"svelte-eslint-parser": "^0.32.1",
		"tailwindcss": "^3.3.3",
		"tslib": "^2.6.1",
		"typescript": "^5.1.6",
		"vite": "^4.4.7",
		"vitest": "^0.34.1"
	},
	"type": "module"
}
