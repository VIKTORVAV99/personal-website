{
	"name": "personal-website",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"build:ci": "vite build --mode production",
		"postbuild:ci": "svelte-sitemap --domain https://viktor.andersson.tech --ignore 404.html",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:ci": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --output machine --fail-on-warnings",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"lint:eslint": "eslint .",
		"format": "prettier --plugin-search-dir . --write . --cache",
		"format:check": "prettier --plugin-search-dir . --check ."
	},
	"devDependencies": {
		"@fortawesome/free-brands-svg-icons": "^6.4.0",
		"@fortawesome/free-solid-svg-icons": "^6.4.0",
		"@skeletonlabs/skeleton": "^1.2.5",
		"@sveltejs/adapter-cloudflare": "^2.2.2",
		"@sveltejs/adapter-static": "^2.0.2",
		"@sveltejs/kit": "^1.15.9",
		"@types/node": "^18.16.3",
		"@typescript-eslint/eslint-plugin": "^5.59.1",
		"@typescript-eslint/parser": "^5.59.1",
		"autoprefixer": "^10.4.14",
		"cssnano": "^6.0.0",
		"eslint": "^8.39.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"postcss": "^8.4.23",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.0",
		"svelte": "^3.58.0",
		"svelte-awesome": "^3.2.0",
		"svelte-check": "^3.3.0",
		"svelte-sitemap": "^2.6.0",
		"tailwindcss": "^3.3.2",
		"tslib": "^2.4.1",
		"typescript": "^5.0.4",
		"vite": "^4.3.3",
		"vitest": "^0.30.1"
	},
	"type": "module"
}
